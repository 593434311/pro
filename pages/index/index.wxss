/**index.wxss**/
.header{
  width:100%;
  height:350rpx;
  background:url(data:image/png;base64,);
  background-size:100% 350rpx;
 
}

swiper-container{
  
}
.swiper-container .swiper{
   width:618rpx;
   padding:24rpx 0;
   margin:0 auto;
}
.swiper-container .swiper-item {
  border-radius: 10rpx;
  overflow: hidden;
}
.swiper-container .swiper image{
 width:618rpx;
  height:328rpx;
}
.swiper-container .dots{
  display: flex;
  justify-content: center;
}
.swiper-container .dots .dot{
  margin: 0 6rpx;
  width: 14rpx;
  height: 14rpx;
  background: #fff;
  border-radius: 50%;
  transition: all .6s;
}
.swiper-container .dots .dot.active{
  width: 14rpx;
  height:14rpx;
  background: #ff3638;
}

/* 拼团 */

.content_tuan{
  /* height:800rpx; */
  background-color:#f9f9fa;
}
.content_tuan .tuan_title{
  padding:69rpx 175rpx 36rpx 173rpx;
  background:#f9f9fa;
}
.content_tuan .tuan_title image{
  width:402rpx;
  height:53rpx;
}

.content_tuan .tuan .tuan_list{
  min-height: 345rpx;
  padding:25rpx;
  display: flex;
  justify-content:space-between;
  margin-top:6rpx;
  background-color:#fff;
}
.content_tuan .tuan .tuan_list:first-child{
  margin-top: 0;
}
.tuan_list .tuan_lt{
  width:245rpx;
  height:245rpx;
}
.tuan_list .tuan_img image{
  width:245rpx;
  height:245rpx;
  border-radius:10rpx;
}
.tuan_list .tuan_rt{
  width:412rpx;
}
.tuan_rt .rt_tit{
  font-size:26rpx;
  color:#a7a7a7;
  margin-bottom:19rpx;
}
.tuan_rt .rt_tit text{
  color:#ff484a;
  margin-right:10rpx; 
}
.tuansw{
  max-height: 690rpx;
  min-height: 600rpx;
}
 .price{
  display:flex;
  align-items:baseline;
  margin-bottom: 25rpx;
}
.price s:nth-of-type(1){
  color:#b8b8b8;
  font-size:20rpx;
  margin-right:20rpx;
}
 .price s:nth-of-type(2){
  color:#ff3638;
  font-size:36rpx;
}

.tuan_rt .count_dn{
  display:flex;
  justify-content: flex-start;
}
.tuan_rt .count_dn text{
  color:#939393;
  font-size:24rpx;
  line-height:44rpx;
  margin-right:19rpx;

}
.count_dn .time{
  font-size:20rpx;
  color:#ff5246;
  text-align: center;
}
.count_dn .time i{
  display:inline-block;
  width:44rpx;
  height:44rpx;
  line-height:44rpx;
  background:linear-gradient(#fbfbfb 50%, #ffffff 50%);
  border-radius:6rpx;
  margin:0 5rpx;
  box-shadow:1rpx 1rpx 29rpx rgba(213,212,210,0.49);
}
.count_dn .time i:nth-of-type(1){
  box-shadow:2rpx 2rpx 6rpx rgba(213,212,210,0.4);
  margin-left:0;
}

.tuan_rt .collect{
  color:#d5d5d5;
  font-size:18rpx;
  display: flex;
  align-items:baseline;
  justify-content:space-between;

}

.want{
  height: 50rpx;
  display: flex;
  align-items:center;
}
 .want image{
  width:17rpx;
  height:15rpx;
  margin-right:6rpx;
}
.collect .purchase{
  width:99rpx;
  height:57rpx;
  
}
.active .more{
   padding:12px 326rpx 84rpx 326rpx; 
}
.active .more image{
  width:99rpx;
  height:57rpx;
}
.load{
  padding:36rpx 326rpx 72rpx 326rpx;
  background:#f9f9fa;
  
}
.load .huan{ 
  font-size:22rpx;
  color:#fff;
  width:98rpx;
  height:50rpx;
  line-height:50rpx;
  text-align: center;
  background:linear-gradient(to right,#fd933d, #fe6e41);
  border-radius:25rpx;
  box-shadow: rgba(254,110,65,0.3) 0 5rpx 3rpx; 
}
.load .tryss { 
  transform: translateX(10rpx);
  font-weight:bold;
  
  
}

/* 热门活动 */
.active .more{
  width:100%;
  height:92rpx;
}


.content_active .active_title{
  
   background:#f9f9fa;
   position:relative;
}
.content_active .active_title image{
  width:373rpx;
  height:52rpx;
   padding:0 186rpx 80rpx 188rpx; 
}
.content_active .active{
  background-color:#f9f9fa;
}
.content_active .active_title text{
   font-size:24rpx;
   color:#b8b8b8;
   position:absolute;
   right:024rpx;
   bottom:22rpx;



}

.coupon .text{
  width: 392rpx;
  margin: 0 auto;
  font-size: 26rpx;
  line-height: 38rpx;
  text-align: center;
  margin-top: 40rpx;
  color: #000;
}
.coupon .coupon_reduce {
  width: 520rpx;
  height: 171rpx;
  background-image: url(data:image/png;base64,);
  background-size: 100%;
  background-repeat: no-repeat;
  margin: 0 auto;
  margin-top: 26rpx;
}
.coupon button{
  width: 288rpx;
  height: 82rpx;
  background: linear-gradient(to right, #fc9f54 , #fe6a3c);
  border: none;
  border-radius: 42rpx;
  color: #fff;
  font-size: 28rpx;
  line-height: 82rpx;
  margin-top: 32rpx;
}
.coupon image{
  width: 100%;
}
.coupon_reduce{
  display: flex;
  flex-direction: row;
}
.coupon_reduce .reduce_left{
  width: 408rpx;
  display: flex;
  justify-content:space-between;
  align-items: center;
  text-align: center;
}
.reduce_left .left_left{
  width: 160rpx;
}
.reduce_left .left_left > text{
  color: #ff3638;
  font-size: 50rpx;
}
.reduce_left .left_left > view{
  color: #b8b8b8;
  line-height: 40rpx;
  font-size: 20rpx;
}
.reduce_left .left_right{
  width: 248rpx;
}
.reduce_left .left_right > view{
  color: #737373;
  font-size: 22rpx;
  line-height: 40rpx;
}
.reduce_left .left_right > text{
  color: #737373;
  font-size: 22rpx;
  background-color: #ffebeb;
  padding: 0 16rpx;
  border-radius: 20rpx;
}
.coupon_reduce .reduce_right{
  width: 112rpx;
  display: flex;
  align-items: center;
}
.coupon_reduce .reduce_right view{
  font-size: 28rpx;
  text-align: center;
  color: #fff;
  width: 20rpx;
  margin: 0 auto;
}
