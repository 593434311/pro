/**index.wxss**/
.header{
  width:100%;
  height:350rpx;
  background:url(data:image/png;base64,);
  background-size:100% 350rpx;
}

swiper-container{
  position: relative;
}
.swiper-container .swiper{
   width:572rpx;
   padding:24rpx 89rpx;
}
.swiper-container .swiper-item {
  border-radius: 10rpx;
  overflow: hidden;
}
.swiper-container .swiper image{
 width:572rpx;
height:303rpx;
}
.swiper-container .dots{
  position: absolute;
  left: 0;
  right: 0;
  top:350rpx;

  display: flex;
  justify-content: center;
}
.swiper-container .dots .dot{
  margin: 0 6rpx;
  width: 14rpx;
  height: 14rpx;
  background: #fff;
  border-radius: 50%;
  transition: all .6s;
}
.swiper-container .dots .dot.active{
  width: 14rpx;
  height:14rpx;
  background: #ff3638;
}

/* 拼团 */

.content_tuan{
  /* height:800rpx; */
  background-color:#f9f9fa;
}
.content_tuan .tuan_title{
  padding:69rpx 175rpx 36rpx 173rpx;
  background:#f9f9fa;
}
.content_tuan .tuan_title image{
  width:402rpx;
  height:53rpx;
}
.content_tuan .tuan .tuan_list{
  padding:25rpx;
  display: flex;
  justify-content:space-between;
  margin-top:6rpx;
  background-color:#fff;
}
.content_tuan .tuan .tuan_list:first-child{
  margin-top: 0;
}
.tuan_list .tuan_lt{
  width:270rpx;
  height:270rpx;
}
.tuan_list .tuan_img image{
  width:270rpx;
  height:270rpx;
  border-radius:10rpx;
}
.tuan_list .tuan_rt{
  width:412rpx;
}
.tuan_rt .rt_tit{
  font-size:26rpx;
  color:#a7a7a7;
  margin-bottom:19rpx;
}
.tuan_rt .rt_tit text{
  color:#ff484a;
  margin-right:10rpx; 
}

 .price{
  display:flex;
  align-items:baseline;
  margin-bottom: 25rpx;
}
.price s:nth-of-type(1){
  color:#b8b8b8;
  font-size:20rpx;
  margin-right:20rpx;
}
 .price s:nth-of-type(2){
  color:#ff3638;
  font-size:36rpx;
}

.tuan_rt .count_dn{
  display:flex;
  justify-content: flex-start;
}
.tuan_rt .count_dn text{
  color:#939393;
  font-size:24rpx;
  line-height:44rpx;
  margin-right:19rpx;

}
.count_dn .time{
  font-size:26rpx;
  color:#ff5246;
  text-align: center;
}
.count_dn .time i{
  display:inline-block;
  width:44rpx;
  height:44rpx;
  line-height:44rpx;
  background:linear-gradient(#fbfbfb 50%, #ffffff 50%);
  border-radius:6rpx;
  margin:0 5rpx;
  box-shadow:1rpx 1rpx 29rpx rgba(213,212,210,0.49);
}
.count_dn .time i:nth-of-type(1){
  box-shadow:2rpx 2rpx 6rpx rgba(213,212,210,0.4);
  margin-left:0;
}

.tuan_rt .collect{
  color:#d5d5d5;
  font-size:18rpx;
  display: flex;
  align-items:baseline;
  justify-content:space-between;

}

 .want{
  display: flex;
  align-items:center;
}
 .want image{
  width:17rpx;
  height:15rpx;
  margin-right:6rpx;
}
.collect .purchase{
  width:99rpx;
  height:57rpx;
  
}
.load{
  padding:36rpx 326rpx 72rpx 326rpx;
  background:#f9f9fa;
}
 .load image{
  width:98rpx;
  height:50rpx;
} 

