/**index.wxss**/
.container{
  background-color: #fff;
}
.banner {
  height: 516rpx;
  width: 750rpx;
  padding:24rpx 89rpx 0 89rpx;
  background-color: #f9f9f9;
  background-image: url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-size: 100%;
  box-sizing: border-box;
}
.swiper-box {
  width: 572rpx;
}

.banner {
  position: relative;
  border-radius: 5rpx;
}
.banner .swiper{
  height: 300rpx;
}
.banner .swiper .img{
  width: 100%;
  height: 100%;
}
.banner .dots{
  position: absolute;
  left: 0;
  right: 0;
  bottom: 162rpx;
  display: flex;
  justify-content: center;
}
.banner .dots .dot{
  margin: 0 8rpx;
  width: 14rpx;
  height: 14rpx;
  background: #ddd;
  border-radius: 8rpx;
  transition: all .6s;
}
.banner image{
  width: 572rpx;
  height: 300rpx;
  border-radius: 5px;
}
.banner .dots .dot.active{
  width: 16rpx;
  background: #ff0000;
}
.kaituan {
  width: 400rpx;
  height: 52rpx;
  margin: 0 auto;
  margin-top: 100rpx;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 100%;
  background-position:center center;
}
.data_ {
  background-color: #f8f8f8;
}
.data_list {
  width: 750rpx;
  display: flex;
  padding: 24rpx;
  flex-direction: row;
  background-color: #fff;
}
.data_list:nth-child(1){
  margin-top: 0;
}
.data_list{
  margin-top: 3rpx;
}
.data_list image{
  border-radius: 5px;
}
.data_list .list_left{
  width: 270rpx;
  height: 270rpx;
}
.data_list .list_right{
  padding-left: 20rpx;
}
.data_list .list_right .GroupName{
  color: #727272;
  font-size: 26rpx;
  line-height: 39rpx;
}
.data_list .list_right .GroupName text{
  color: #ff5246;
}
.data_list .price text{
  color: #b8b8b8;
  font-size: 20rpx;
}
.data_list .price text text{
  color: #ff5246;
  font-size: 36rpx;
}

.list_time >text{
  color: #939393;
  font-size: 24rpx;
}
.list_time {
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 13rpx;
}

.list_time .time_{
  display: flex;
  align-items: center;
  color: #ff5246;
  margin-left: 10rpx;
  font-size: 30rpx;
}
.list_time .time_ text{
  width: 40rpx;
  height: 40rpx;
  border-radius: 4px;
  border: 1px solid #ddd;
  margin: 0 6rpx;
  box-shadow: #d5d4d2 0rpx 0rpx 15rpx;
  text-align: center;
  line-height: 40rpx;
  font-size: 26rpx;
}
.list_right .like{
  display: flex;
  align-items: center;
  justify-content: space-between;
  color: #d5d5d5;
  font-size: 18rpx;
}
.list_right .like .like_q {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.like .like_q .ioc{
  width: 18rpx;
  height: 18rpx;
  background-repeat: no-repeat;
  background-size: 100%;
  margin-right: 2rpx;
}
.cart{
  width: 88rpx;
  height: 50rpx;
  background-repeat: no-repeat;
  background-size: 100%;
  background-image: url(data:image/png;base64,)
}
.wontGo {
  background-image: url(data:image/png;base64,)
}
.nowongGo {
  background-image: url(data:image/png;base64,)
}
.main .active {
  padding-top: 36rpx;
  background-color: #f9f9f9;
}
.main .active .requer{
  width: 99rpx;
  height: 57rpx;
  margin: 0 auto;
  margin-bottom: 72rpx;
  background-repeat: no-repeat;
  background-size: 100%;
  background-image: url(data:image/png;base64,);
}
.main .active .hot_active {
  width: 373rpx;
  height: 52rpx;
  margin: 0 auto;
  margin-bottom: 36rpx;
  background-repeat: no-repeat;
  background-size: 100%;
  background-image: url(data:image/png;base64,);
}
.active .active_ .active_list{
  padding: 24rpx;
  background-color: #fff;
}
.active .active_ .active_list image{
  border-radius: 4px;
  width: 702rpx;
  height: 355rpx;
}