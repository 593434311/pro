/* pages/find/index/index.wxss */
 page{
   background-color: #f9f9fa;
 }
 .tuan_title{
  padding:37rpx 175rpx 30rpx 173rpx;
  background:#f9f9fa;
}
 .tuan_title image{
  width:402rpx;
  height:53rpx;
}
.gengduo{
   width: 99rpx;
   margin: 0 auto;
   margin-top: 35rpx;
}
.gengduo image{
  width: 99rpx;
  height: 57rpx;
}
.counter > view{
  display: flex;
  flex-direction: row;
  padding: 24rpx;
  background-color: #fff;
  margin-bottom: 2rpx;
}
.list_boximg image{
  width: 124rpx;
  height: 124rpx;
  border-radius: 5px;
}
.box_text{
  position: relative;
  width: 100%;
  margin-left: 18rpx;
  font-size: 26rpx;
  color: #7b7b7b;
}
.cont {
  position: absolute;
  left: 0;
  bottom: 10rpx;
}
.quality{
  background-color: #fff;
}
.quality > .tit{
  display: flex;
  flex-direction: row;
  padding: 24rpx;
  padding-bottom: 14rpx;
  margin-bottom: 2rpx;
}
.quality_boximg image{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}
.qualityname{
  display: flex;
  margin-top:5rpx;
  align-items: center;
  flex-direction: row;
  color: #7b7b7b;
  font-size: 30rpx;
}
.quality_text{
  margin-left: 20rpx;
}
.qualityname text{
  width: 55rpx;
  height: 26rpx;
  margin-left: 12rpx;
  background: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 100%;
}
.Label{
  font-size: 24rpx;
  color: #ffa4a5;
  margin-top: 20rpx;
}
.tex_img{
  padding: 0 24rpx;
  padding-bottom: 40rpx;
}
.tex_img .tit{
  color: #7b7b7b;
  font-size: 26rpx;
}
.tex_img .img{
  margin-top: 24rpx;
}
.tex_img .img image{
  width: 216rpx;
  height: 216rpx;
  border-radius: 5px;
  margin-left: 27rpx;
}
.tex_img .img image:first-child{
  margin-left: 0;
}