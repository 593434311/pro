/* pages/find/articledetails/index.wxss */
.head{
  background-color: #fff;
  padding: 24rpx;
  
}
.articlshei{
  color: #737373;
  font-size: 36rpx;
}
.pulltime{
  font-size: 22rpx;
  color: #b8b8b8;
  margin-top: 20rpx;
  text-align: right;
}
.counter{
  padding: 36rpx 24rpx;
  margin-top: 14rpx;
  background-color: #fff;
}
.counter view{
  line-height: 36rpx;
  font-size: 24rpx;
  color: #8d8d8d;
}
.end{
  width: 100%;
  padding: 0 24rpx;
  margin-top: 40rpx;
  background-repeat: no-repeat;
  height: 18rpx;
  background: url(data:image/png;base64,);
  background-size: 100%;
}
.fdex{
  position: fixed;
  bottom: 200rpx;
  right: 24rpx;
  display: flex;
  flex-direction: row;
}
.like image{
  width: 42rpx;
  height: 37rpx;
  margin-right: 12rpx;
}
.like{
  height: 80rpx;
  min-width: 160rpx;
  padding: 0 29rpx;
  display: flex;
  align-items: center;
  background-color: #e5e5e5;
  border-radius: 40rpx;
  margin-right: 38rpx;
}
.shere button{
  width: 84rpx;
  height: 84rpx;
  border-radius: 50%;
  background-color: #fe5354;
  display: flex;
  align-items: center;
  justify-content: center;
}
.shere image{
  width: 26rpx;
  height: 30rpx;
}