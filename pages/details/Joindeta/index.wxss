/* pages/details/Joindeta/index.wxss */
page{
  background-color: #fff;
}
.leader{
  position: relative;
}
.leader::after{
  position: absolute;
  left: 70rpx;
  top: 40rpx;
  content: "团长";
  height: 24rpx;
  background-color:#ff3638;
  color:#fff;
  border-radius:12rpx;
  font-size:18rpx;
  padding:0 7rpx;
  line-height: 24rpx;
}
.member{
  padding: 48rpx 65rpx; 
  background-color: #fff;
  display: flex;
  flex-wrap: wrap;
}
.member view{
  width: 100rpx;
  height: 100rpx;
  margin-right: 30rpx;
  margin-bottom: 24rpx;
  border-radius: 50%;
}
.member view:nth-child(5n){
  margin-right: 0
}
.member view image{
  border-radius: 50%;
  width: 100%;
  height: 100%;
}
.yaoqbg{
  background-color: #f9f9f9;
  height: 12rpx;
}
.yaoq {
  padding-top:74rpx;
  text-align: center;
}

.tuan_rt .count_dn{
  display:flex;
  justify-content: flex-start;
}
.count_dn .time{
  display: flex;
  margin-top: 24rpx;
  align-items: center;
  justify-content:center;
  font-size: 26rpx;
  font-weight: bold;
  color:#ff5246;
  text-align: center;
}
.tit_text2{
  color:#8d8d8d;
  font-size:26rpx;
  margin: 40rpx 0;
}
.tit_text2 text{
  display: inline-block;
  text-align: center;
  line-height:54rpx;
  margin: 0 9rpx; 
  width:32rpx;
  height:39rpx;
  color:pink;
  font-size:24rpx;
  background-image:url(data:image/png;base64,);
  background-repeat:no-repeat;
  background-size:100%; 
}
.count_dn .time{
  color:#ccc;
}
.count_dn .time i{
  display:inline-block;
  color: red;
  width:44rpx;
  height:44rpx;
  line-height:44rpx;
  background:linear-gradient(#fbfbfb 50%, #ffffff 50%);
  border-radius:6rpx;
  margin:0 5rpx;
  box-shadow:1rpx 1rpx 29rpx rgba(213,212,210,0.49);
}
.count_dn .time i:nth-of-type(1){
  box-shadow:2rpx 2rpx 6rpx rgba(213,212,210,0.4);
  margin-left:0;
}
.kaitButton {
  font-size: 28rpx;
  color: #fff;
  width: 424rpx;
  height: 84rpx;
  display: flex;
  align-items: center;
  border-radius: 42rpx;
  background: linear-gradient(to bottom, #fc9f54 , #fe6a3c);
  justify-content: center;
}
.shea{
  display: inline-block;
  width: 26rpx;
  height: 30rpx;
  background: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 100%;
  margin-right: 25rpx;
}