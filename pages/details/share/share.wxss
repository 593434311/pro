/* pages/details/share/share.wxss */
page{
  width:100%;
  background-image:url(http://gtshidai.oss-cn-shanghai.aliyuncs.com/shop/1/15398487116973.png);
  background-size:100%;
}
.warp{
  padding:680rpx 24rpx 0;
}
.warp .btn_jion{
  width:340rpx;
  height:100rpx;
  line-height:100rpx;
  color:#fff;
  font-size:36rpx;
  text-align: center;
  margin:0 auto;
  background:linear-gradient(#fd9f53,#fe6b3c);
  border-radius:50rpx;
}
.content{
  color:#fff;
  text-align: center;
}
.content .text_con{
  padding:63rpx 0 140rpx 0;
}
.content .text_con>text{
  display:block;
  font-size:24rpx;
  line-height:42rpx;
  
}
.text_con .money text{
  color:#fe4755;
  font-size:24rpx;
  line-height:42rpx;
}
.content .text_footer{
  font-size:28rpx;
}
.coupon .text{
  width: 392rpx;
  margin: 0 auto;
  font-size: 26rpx;
  line-height: 38rpx;
  text-align: center;
  margin-top: 40rpx;
  color: #000;
}
.coupon .coupon_reduce {
  width: 520rpx;
  height: 171rpx;
  background-image: url(data:image/png;base64,);
  background-size: 100%;
  background-repeat: no-repeat;
  margin: 0 auto;
  margin-top: 26rpx;
}
.coupon button{
  width: 288rpx;
  height: 82rpx;
  background: linear-gradient(to right, #fc9f54 , #fe6a3c);
  border: none;
  border-radius: 42rpx;
  color: #fff;
  font-size: 28rpx;
  line-height: 82rpx;
  margin-top: 32rpx;
}
.coupon image{
  width: 100%;
}
.coupon_reduce{
  display: flex;
  flex-direction: row;
}
.coupon_reduce .reduce_left{
  width: 408rpx;
  display: flex;
  justify-content:space-between;
  align-items: center;
  text-align: center;
}
.reduce_left .left_left{
  width: 160rpx;
}
.reduce_left .left_left > text{
  color: #ff3638;
  font-size: 50rpx;
}
.reduce_left .left_left > view{
  color: #b8b8b8;
  line-height: 40rpx;
  font-size: 20rpx;
}
.reduce_left .left_right{
  width: 248rpx;
}
.reduce_left .left_right > view{
  color: #737373;
  font-size: 22rpx;
  line-height: 40rpx;
}
.reduce_left .left_right > text{
  color: #737373;
  font-size: 22rpx;
  background-color: #ffebeb;
  padding: 0 16rpx;
  border-radius: 20rpx;
}
.coupon_reduce .reduce_right{
  width: 112rpx;
  display: flex;
  align-items: center;
}
.coupon_reduce .reduce_right view{
  font-size: 28rpx;
  text-align: center;
  color: #fff;
  width: 20rpx;
  margin: 0 auto;
}
