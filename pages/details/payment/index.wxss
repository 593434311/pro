/* pages/details/payment/index.wxss */
.payment{
  
}
.pay_header{
  height: 226rpx;
  width: 100%;
  padding: 26rpx;
  text-align: center;
  background: linear-gradient(#fe6e41, #fd9e51);
}
.pay_header image {
  width: 140rpx;
  height: 118rpx;
}
.pay_header text {
  display: block;
  margin-top: 10rpx;
  color: #fff;
  font-size: 28rpx;
}
.pay_counter{
  background-color: #fff;
  text-align: center;
  padding-top:24rpx;
  padding-bottom: 45rpx;
}
.pay_counter .Countdown_te{
  display: block;
  font-size: 24rpx;
 
  margin-bottom: 17rpx;
}
.pay_counter .Countdown{
  display: flex;
  justify-content: center;
  line-height: 71rpx;
}
.Countdown view {
  width: 71rpx;
  height: 71rpx;
  background: url(data:image/png;base64,);
  background-size: 100%;
  background-repeat: no-repeat;
  color: #fe374e;
  font-weight: bold;
}
.Countdown text{
  color: #ff8688;
}
.difference{
  font-size: 26rpx;
  line-height: 50rpx;
  margin: 15rpx 0;
  margin-bottom: 36rpx;
}
.difference text {
  display: inline-block;
  width: 32rpx;
  height: 39rpx;
  color: #fff;
  font-size: 21rpx;
  margin: 0 12rpx;
  background: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 100%;
}
.count_butt button{
  display: flex;
  width: 422rpx;
  height: 84rpx;
  margin: 0 auto;
  color: #fff;
  border-radius: 42rpx;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  background: linear-gradient(#fd9e51, #fe6e41);
}
.count_butt view{
  width: 26rpx;
  height: 30rpx;
  background: url(data:image/png;base64,);
  background-size: 100%;
  background-repeat: no-repeat;
  margin-right: 25rpx;
}
.offered {
  margin-top: 14rpx;
  background-color: #fff;
  padding: 0 24rpx;
}
.rule {
  display: flex;
  justify-content:space-between;
  align-items: center;
  margin-top: 2rpx;
  background-color: #fff;
  height: 90rpx;
  padding: 0 24rpx;
}
.rule_left{
  display: flex;
  font-size: 26rpx;
  align-items: center;
}
.rule_left .bg{
  display: block;
  width: 26rpx;
  height: 29rpx;
  margin-right: 15rpx;
  background: url(data:image/png;base64,);
  background-size: 100%;
  background-repeat: no-repeat;
}
.rule_right{
  color: #ff373a;
  font-size: 22rpx;
}
.integral{
  text-align: center;
  margin-top: 12rpx;
  padding: 32rpx 65rpx;
  background-color: #fff;
}
.integral .tit{
  color: #ff7477;
  font-size: 36rpx;
  line-height: 54rpx;
}
.integral .tit_{
  font-size: 24rpx;
  margin-bottom: 25rpx;
}
.img_t{
  height: 106rpx;
  border-radius: 5px;
  display: flex;
  align-items: center;
  justify-content: center;
  background: url(data:image/png;base64,);
  background-size: 100%;
  background-repeat: no-repeat;
}
.img_t > view {
  width: 360rpx;
  height: 70rpx;
  display: flex;
  align-items: center;
  color: #f27b38;
  font-size: 28rpx;
  line-height: 70rpx;
  border-radius: 5px;
  background-color: #fff;
}
.img_t .img_bg {
  display: block;
  width: 26rpx;
  height: 29rpx;
  margin-right: 15rpx;
  background: url(data:image/png;base64,);
  background-size: 100%;
  background-repeat: no-repeat;
  margin:0 16rpx 0 26rpx;
}
.text__{
  margin-top: 30rpx;
  font-size: 22rpx;
  line-height: 36rpx;
}