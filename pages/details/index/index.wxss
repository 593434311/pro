/* tabBar */

.bot_tab{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 750rpx;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content:space-between;
  background-color: #fff;
  box-sizing: border-box;
  border: none;
  border-top: 1rpx solid #ccc;
  padding: 0 24rpx;
}
button::after{ border: none; }
.bot_tab .bot_left {
  width: 200rpx;
}
.bot_tab .bot_left{
  display: flex;
  flex-direction: row;
  align-items: center;
  color: #737373;
}
.bot_tab .bot_left>view,.bot_tab .bot_left>button {
  height: 75rpx;
  text-align: center;  
}
.bot_tab .bot_left > button{
  background: none;
  position: static;
  border: none;
  color: #737373;
  overflow:initial;
}
.bot_tab .bot_left>view view,.bot_tab .bot_left>button view{
  font-size: 18rpx;
  margin-top: 10rpx;
  line-height: 20rpx;
  text-align: center;
}
.bot_tab .Placeanorder {
  width: 445rpx;
}
