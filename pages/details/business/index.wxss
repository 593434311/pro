/* pages/details/business/index.wxss */
page{
  height: 100%;
  background-color: #f9f9fa;
}
.bm_bottom{
  padding:24rpx 22rpx;
  margin-top: 17rpx;
  color:#b8b8b8;
  font-size:24rpx;
  line-height: 36rpx;
}
.bm_bottom .location{
  border-top: 1px solid #e5e5e5;
  padding-bottom:20rpx;
  padding-top:20rpx;
  position: relative;
  padding-left:32rpx;
}
.bm_bottom .location::before{
  content: '';
  width: 20rpx;
  height: 24rpx;
  background-image:url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 100%;
  position: absolute;
  left: 0;
  top: 26rpx ;
}
.bm_bottom .location image{
  width:20rpx;
  height:24rpx; 
}
.bm_bottom .time image{
  width:20rpx;
  height:20rpx;
  padding-right:8rpx; 
}
.top_img{
  width: 750rpx;
  height: 390rpx;
}
.headrer{
  position: relative;
  background-color: #fff;
}
.portrait{
  position: absolute;
  top: 336rpx;
  left: 321rpx;
  width: 108rpx;
  height: 108rpx;
  border-radius: 50%;
  padding: 5rpx;
  background-color: #fff;
  text-align: center
}
.portrait image{
  width: 98rpx;
  height: 98rpx;
  border-radius: 50%;
}
.user_name{
  margin-top: 88rpx;
  text-align: center;
  color: #737373;
  font-size: 26rpx;
}
.toggle{
  width: 498rpx;
  margin: 0 auto;
  margin-top: 48rpx;
  margin-bottom: 24rpx;
  display: flex;
  justify-content: space-between;
}
.toggle view{
  width: 196rpx;
  height: 68rpx;
  border-radius: 25rpx;
  color: #fff;
  text-align: center;
  line-height: 68rpx;
  font-size: 22rpx;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 100%;
}
.toggle .selet{
  background-image: url(data:image/png;base64,);
}
.toggle_data{
  padding: 24rpx;
  background-color: #fff;
}
.toggle_data .active{
  padding-bottom: 100rpx;
}
.fdex{
  position: fixed;
  bottom: 100rpx;
  right: 24rpx;
  display: flex;
  flex-direction: row;
}
.like image{
  width: 42rpx;
  height: 37rpx;
  margin-right: 12rpx;
}
.like{
  height: 80rpx;
  min-width: 160rpx;
  padding: 0 29rpx;
  display: flex;
  align-items: center;
  justify-content:center;
  background-color: #fff;
  border:1rpx solid #fe5354;
  border-radius: 40rpx;
  margin-right: 38rpx;
 

}
.shere button{
  width: 84rpx;
  height: 84rpx;
  border-radius: 50%;
  background-color: #fe5354;
  display: flex;
  align-items: center;
  justify-content: center;
}
.shere image{
  width: 26rpx;
  height: 30rpx;
}