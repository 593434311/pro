/* 购买数量 */


.quantity .tit_quantity{
  font-size:26rpx;
  color:#737373; 
}
.quantity .quantity_cont{
  text-align:right;
  
}
.quantity .return_money{
  display:inline-block;
  width: 100%;
  font-size:20rpx;
  color:#b8b8b8;
  padding-bottom:23rpx;
}
.price{
   line-height:42rpx;
   padding: 16rpx 0 13rpx 0;
}
.price>text{
   color:#b8b8b8;
   font-size: 20rpx;
   margin-right:30rpx;
}
.price .group_price{
   color:#737373;
   font-size: 26rpx;
   display:inline-block;
}
.price .group_price text{
   color:#ff5646;
   font-size: 36rpx;
}
