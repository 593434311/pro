/* pages/personal/index/index.wxss */
.contentW{
  width: 100%;
  overflow: hidden;
  padding: 0 24rpx;
  background-color:#fff;
}
.bottom_border{
  position: relative;
}
.bottom_border::after{
  position: absolute;
  left: -20rpx;
  bottom: 0;
  content: "";
  width:750rpx;
  height: 2rpx;
  background-color:#f2f2f2;
}

.unpaid{
  padding:24rpx 66rpx 38rpx;
  display:flex;
  justify-content:space-between;
  align-items:center;
   background:linear-gradient(to right,orangered,orange);
}
.unpaid .unpaid_lt{
  text-align: center;
}

.unpaid .unpaid_lt image{
  width:134rpx;
  height:106rpx;
  padding-bottom:34rpx;
}
.unpaid .unpaid_lt text{
  display: block;
  color:#fff;
  font-size:24rpx;
}
.unpaid  .unpaid_rt{
  position:relative;
  padding-left:50rpx;
  width:400rpx;
}
.unpaid  .unpaid_rt::before{
  content:"";
  width:1rpx;
  height:100rpx;
  background-color:#feb08f;
  position:absolute;
  top:0;
  left:0;
}
.unpaid  .unpaid_rt text{
  color:#fff;
  font-size:28rpx;
  height:42rpx;

}
/* 参团详情 */
.tuan_detail{

}
.information_cont .text_tit{
  width:100rpx;
   line-height: 28rpx;
  /* display: flex;
  align-items:center; */
}
 .title {
  display:flex;
  justify-content:space-between;
  align-items:baseline;
  padding:28rpx 0 35rpx 0;
}
.title  .cantuan{
   display:flex;
  align-items:baseline;
}
.title  image{
  width:32rpx;
  height:29rpx;
  margin-right:10rpx;
}
.title .tit_text1{
  color:#4c4c4c;
  font-size:30rpx;
}
.title  .tit_text2{
  color:#8d8d8d;
  font-size:24rpx;
}

.title  .tit_text2 text{
  display: inline-block;
  text-align: center;
  line-height:54rpx;
  margin: 0 9rpx; 
  width:32rpx;
  height:39rpx;
  color:#fff;
  font-size:24rpx;
  background-image:url(data:image/png;base64,);
  background-repeat:no-repeat;
  background-size:100%; 
}

.head_portrait{
  padding-bottom:50rpx;
  display: flex;
  justify-content:space-between;
  align-items:center;
  
}
.head_portrait .head_img{
  padding:0 42rpx;
  display: flex;

}
.head_img .touxiang{
  padding-right:14rpx;
}
.head_img .touxiang image{
  width:87rpx;
  height:87rpx;
}
.head_portrait .head_more image{
  width:12rpx;
  height:22rpx;
}
/* 信息填写 */
.information{

}
.information .information_cont{
  display: flex;
  align-items:center;
  padding:30rpx 42rpx;
  line-height: 28rpx;
  font-size:28rpx;
  color:#8d8d8d;
}
.information .information_cont input{
  color:#b8b8b8;
  padding-left:58rpx;
}

/* 购买数量 */


.quantity .tit_quantity{
  font-size:26rpx;
  color:#737373; 
}
.quantity .quantity_cont{
  text-align:right;
  
}
.quantity .return_money{
  display:inline-block;
  width: 100%;
  font-size:20rpx;
  color:#b8b8b8;
  padding-bottom:23rpx;
}
.price{
   line-height:42rpx;
   padding: 16rpx 0 13rpx 0;
}
.price>text{
   color:#b8b8b8;
   font-size: 20rpx;
   margin-right:30rpx;
    text-decoration:line-through;
}
.price .group_price {
   color:#737373;
   font-size: 26rpx;
   display:inline-block;
   
}

.price .group_price text{
   color:#ff5646;
   font-size: 36rpx;
   
    
}
/* 优惠券 */
.coupon{
}
.coupon .money_off{

}
.coupon .money_off text{
  color:#ffa2a3;
  font-size:26rpx;
}
.coupon .money_off image{
  width:12rpx;
  height:22rpx;
  margin:0 0 0 14rpx;
  
}
.coupon .payment{
  text-align:right;
