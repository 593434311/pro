}
/* 底部 */
.bottom{
  position:fixed;
  left:0;
  bottom:0;
  width:750rpx;
  height:100rpx;
  background-color:#fff;
}
.bottom .bottom_cont{
  padding:0 24rpx;
  display: flex;
  justify-content:flex-end;
  align-items:center;
}
.bottom .bottom_cont .bottom_text{
  color:#8d8d8d;
  font-size: 26rpx;
}
.bottom_cont .bottom_text text{
  color:#ff5246;
  padding-right:24rpx;
}
.bottom_cont .bottom_pay{
  height:84rpx;
  color:#fff;
  font-size:24rpx;
  text-align: center;
  line-height:84rpx;
  padding:0 94rpx;
  background:linear-gradient(#ff7f9d,#ff3739);
  border-radius:42rpx;
  
}