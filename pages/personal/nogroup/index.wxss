/* pages/personal/index/index.wxss */
.contentW{
  padding: 0 24rpx;
  background-color:#fff;
  margin-bottom:12rpx;
}
.bottom_border{
  position: relative;
}
.bottom_border::after{
  position: absolute;
  left: -20rpx;
  bottom: 0;
  content: "";
  width:750rpx;
  height: 2rpx;
  background-color:#f2f2f2;
}

.unpaid{
  padding:24rpx 66rpx 38rpx;
  display:flex;
  justify-content:space-between;
  align-items:center;
   background:linear-gradient(to right,orangered,orange);
}
.unpaid .unpaid_lt{
  text-align: center;
}

.unpaid .unpaid_lt image{
  width:134rpx;
  height:106rpx;
  padding-bottom:34rpx;
}
.unpaid .unpaid_lt text{
  display: block;
  color:#fff;
  font-size:24rpx;
}
.unpaid  .unpaid_rt{
  width: 400rpx;
  position:relative;
  padding-left:50rpx;
}
.unpaid_rt text{
  display: block
}
.unpaid .unpaid_rt text:first-child{
  font-size: 28rpx;
}
.unpaid .unpaid_rt text:last-child{
  font-size: 24rpx;
}
.unpaid  .unpaid_rt::before{
  content:"";
  width:1rpx;
  height:100rpx;
  background-color:#feb08f;
  position:absolute;
  top:0;
  left:0;
}
.unpaid  .unpaid_rt text{
  color:#fff;
  font-size:28rpx;
  height:42rpx;

}
