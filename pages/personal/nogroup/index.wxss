/* 底部 */
.bottom{
  position:fixed;
  left:0;
  bottom:0;
  width:750rpx;
  height:100rpx;
  background-color:#fff;
}

.bottom_cont {
  display: flex;
  padding: 18rpx 0;
  margin-right: 24rpx;
  justify-content:  flex-end;
}
.bottom_cont button{
  width: 130rpx;
  height: 50rpx;
  margin-left: 20rpx;
  font-size: 20rpx;
  color: #b8b8b8;
  line-height: 50rpx;
  background-color: #fff;
  border: 1rpx solid #b8b8b8;
  border-radius: 25rpx;
}
.text_orde{
  background-color: #fff;
  margin-bottom: 100rpx;
  color: #a7a7a7;
  line-height: 42rpx;
  padding: 24rpx 24rpx;
}
.text_orde view{
  font-size: 18rpx;
}
.fuzi{
  color: #ff9394;
  display: inline-block;
  margin-left: 93rpx;
}
.order_box{
  background-color: #fff;
